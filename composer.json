{
    "name": "craffft/api-client-bundle",
    "type": "contao-bundle",
    "description": "API Clients with Token Authentication for Contao",
    "keywords": ["api", "client", "token", "contao"],
    "homepage":"https://craffft.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Kiesel",
            "homepage": "https://github.com/iCodr8"
        }
    ],
    "support": {
        "issues": "https://github.com/Craffft/api-client-bundle/issues",
        "source": "https://github.com/Craffft/api-client-bundle"
    },
    "require": {
        "php": ">=5.4.0",
        "contao/core-bundle": "~4.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "extra": {
        "contao-manager-plugin": "Craffft\\ApiClientBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Craffft\\ApiClientBundle\\": "src/"
        }
    }
}
